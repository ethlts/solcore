data Food = Curry | Beans | Other
data CFood = Red[Food] | Green[Food] | Nocolor



  function fromEnum(x : CFood) {
     match x {
       | Red[Curry] => return 1;
       | Green[Beans] => return 2;
       | _ => return 3;
     };
  }


contract Food {
  function id(x) {
    return(x);
  }
  
  function main() {
  return fromEnum(id(Green[Beans]));
  }
}
