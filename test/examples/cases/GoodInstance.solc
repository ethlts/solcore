class a:Enum {
    function fromEnum(x:a) -> Word;
}

  data Color = R | G | B

instance Color : Enum {
  function fromEnum(c) {
    match c {
      | R => return 1;
      | G => return 2;
      | B => return 3;
    };
  }
}


data Bool = False | True

instance Bool : Enum {
  function fromEnum(b) {
      match b {
      | False => return 0;
      | True => return 1;
      };
  }
}

contract GoodInstance {
  function main() { return fromEnum(True);}
}
