contract Option {
  data Option(a) = None | Some(a);
  data Bool = False | True;

  function maybe(n, o) {
    match o {
      | None => return n;
      | Some(x) => return x;
    };
  }

  function join(mmx) {
    let result = None;
    match mmx {
      | Some(Some(x)) => result = Some(x);
      | None => result = None;
    };
    return result;
  }

  
  function main() {
    return maybe(0, join(Some(Some(False))));
  }
}
